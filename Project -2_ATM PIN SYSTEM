while True:
    balance = 50000
    pin = 1234
    user_pin = int(input("Enter your pin"))
    
    if user_pin ==pin:
        print("Your balance is:",balance)
        amount = int(input("Enter amount to withdrawal:"))
        balance-=amount
        print("Congrate Amount recived", "New balance",balance)
    else :
        print("Insufficient Balance")
        break
   
